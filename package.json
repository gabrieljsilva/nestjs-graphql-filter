    "premajor": "npm version premajor --preid=rc",
    "preminor": "npm version preminor --preid=rc",
    "prepatch": "npm version prepatch --preid=rc",
    "prerelease": "npm version prerelease --preid=rc"
  },
  "optionalDependencies": {
    "@gabrieljsilva/nestjs-graphql-filter-adapter-prisma": "^0.1.0-rc.2"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.10",
    "@nestjs/core": "^9.3.10",
    "@nestjs/graphql": "^11.0.4",
    "graphql": "^16.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-morph": "^17.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.0.0",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
