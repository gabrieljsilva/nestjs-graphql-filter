  "description": "Generate filters for Nestjs GraphQL package based on decorators",
  "author": "gabrieljsilva",
  "private": false,
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "major": "npm version major",
